[
  {
    "name": "starts with separator",
    "input": {"status": "ok"},
    "starts_with": "---\n"
  },
  {
    "name": "strings quoted",
    "input": {"name": "hello", "count": 42, "ok": true},
    "contains": ["name: \"hello\"", "count: 42", "ok: true"]
  },
  {
    "name": "null",
    "input": {"value": null},
    "contains": ["value: null"]
  },
  {
    "name": "nested",
    "input": {"config": {"api_key_secret": "***", "dns_ttl_s": 3600}},
    "contains": ["config:\n", "  api_key_secret: \"***\"", "  dns_ttl_s: 3600"]
  },
  {
    "name": "array",
    "input": {"tags": ["a", "b"]},
    "contains": ["tags:\n", "  - \"a\"", "  - \"b\""]
  },
  {
    "name": "empty array",
    "input": {"items": []},
    "contains": ["items: []"]
  },
  {
    "name": "values not transformed",
    "input": {"latency_ms": 42, "size_bytes": 456789},
    "contains": ["latency_ms: 42", "size_bytes: 456789"]
  },
  {
    "name": "string with newline escaped",
    "input": {"msg": "hello\nworld"},
    "contains": ["msg: \"hello\\nworld\""]
  },
  {
    "name": "string with tab escaped",
    "input": {"data": "a\tb"},
    "contains": ["data: \"a\\tb\""]
  }
]
